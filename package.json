{
    "name": "blockexplorer",
    "version": "1.0.0",
    "scripts": {
        "build": "PUBLIC_PATH=https://suite.camino.network/explorer/ webpack --config webpack.prod.js",
        "build:dev": "PUBLIC_PATH=https://dev.suite.camino.network/explorer/ webpack --config webpack.prod.js",
        "build:stage": "PUBLIC_PATH=https://stage.suite.camino.network/explorer/ webpack --config webpack.prod.js",
        "build:start": "cd dist && PORT=5002 npx serve",
        "start": "webpack-dev-server --config webpack.dev.js",
        "start:live": "webpack-dev-server --open --config webpack.dev.js --live-reload",
        "checkTs": "tsc --noEmit",
        "eslint": "eslint --ext js,ts,tsx",
        "lint": "yarn run eslint src",
        "lint:fix": "yarn run eslint --fix src",
        "prepare": "husky install",
        "format-check": "prettier --check '{*.js, src/**/*.{js,jsx,ts,tsx}, .github/workflows/*.yml}'",
        "format": "prettier --write '{*.js, src/**/*.{js,jsx,ts,tsx}, .github/workflows/*.yml}'"
    },
